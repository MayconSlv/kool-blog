{
  "name": "kool-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --outDir ./dist",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm migration:run -- -d ./src/data/db/config.ts",
    "migrate:test": "npm run typeorm migration:run -- -d ./src/data/db/config.ts -- --dotenv dotenv_config_path=.env.test",
    "migrate:generate": "npm run typeorm migration:generate ./src/data/db/migration/addNewsColumns -- -d ./src/data/db/config.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "start": "npm run build && node -r ./tsconfig-paths-dist.js dist/api/index.js",
    "start:dev": "tsc-watch --onSuccess \"node -r ./tsconfig-paths-dist.js dist/api/index.js --inspect\" ",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register --exit 'src/**/*.test.ts' --dotenv dotenv_config_path=.env.test"
  },
  "keywords": [],
  "author": "mayconslv <mayconslv.contato@gmail.com>",
